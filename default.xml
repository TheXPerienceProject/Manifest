<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <!-- AOSP -->
  <remote name="aosp"
        fetch="https://android.googlesource.com"
        review="android-review.googlesource.com"
        sync-c="true"
        sync-tags="false"
        revision="refs/tags/android-13.0.0_r1" />

  <!-- CLO -->
  <remote name="clo_system"
          fetch="https://git.codelinaro.org/clo/la"
          sync-c="true"
          sync-tags="false"
          review="codelinaro.org"
          revision="refs/tags/LA.QSSI.13.0.r1-06400-qssi.0" />

  <!--XPerience Stuff-->
  <remote name="xpe"
          fetch=".."
          review="https://github.com/TheXPerienceProject/"
          revision="refs/heads/xpe-17.0" />

  <remote  name="gitlab"
           fetch="https://gitlab.com"
           revision="xpe-17.0" />

  <remote  name="unicorn"
           fetch="https://github.com/RainbowUnicornXPe"
           revision="xpe-17.0" />

  <remote  name="unicornlab"
           fetch="https://gitlab.com/RainbowUnicornXPe"
           revision="xpe-17.0" />

  <remote  name="github"
           fetch="https://github.com/" />

  <remote  name="blobs-pa"
           fetch="https://github.com/ThankYouMario"
           revision="topaz" />

  <remote  name="blobs-gitlab-pa"
           fetch="https://gitlab.com/ThankYouMario"
           revision="topaz" />

  <remote  name="lineage"
           fetch=".."
           review="review.lineageos.org"
           revision="refs/heads/lineage-20.0" />

  <default revision="xpe-17.0"
           remote="xpe" />

  <include name="snippets/aosp.xml"/>
  <include name="snippets/remove.xml"/>
  <include name="snippets/xperience.xml"/>

</manifest>
